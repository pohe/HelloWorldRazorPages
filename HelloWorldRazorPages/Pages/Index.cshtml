@page
@model IndexModel
@{
    ViewData["Title"] = "Hej alle ";
}

<div class="text-center">
    <h1 class="display-4">Welcome to Razor pages</h1>
    <p>Learn  about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    
    
    <form method="Post">
        <label asp-for="@Model.Car.Name" class="control-label"></label>
        <input asp-for="@Model.Car.Name" class="form-control"/>
        <input type="submit" value="Test Post request"/>
    </form>
    @for (int i = 0; i < 10; i++)
    {

        <p>@i@Model.Message</p>
        <p>@Model.Name</p>
        <p>@(
               $"Bilens navn {Model.Car.Name} årstal {Model.Car.Year}" 
           )</p>
    }
    
</div>
